{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@wpcampus/wpcampus-wc-blog": "^0.0.5",
    "@wpcampus/wpcampus-wc-conduct": "^0.0.2",
    "@wpcampus/wpcampus-wc-footer": "^0.0.3",
    "@wpcampus/wpcampus-wc-notifications": "^0.0.4",
    "@wpcampus/wpcampus-wc-tweets": "^0.0.2",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.19.45",
    "gatsby-cli": "^2.11.0",
    "gatsby-image": "^2.2.30",
    "gatsby-plugin-manifest": "^2.2.25",
    "gatsby-plugin-offline": "^3.0.17",
    "gatsby-plugin-postcss": "^2.1.20",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-source-filesystem": "^2.1.35",
    "gatsby-source-wordpress": "^3.1.47",
    "gatsby-transformer-sharp": "^2.3.1",
    "node-fetch": "^2.6.0",
    "postcss-nested": "^4.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-html-parser": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^1.18.2",
    "stylelint": "^13.2.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "playground": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint": "eslint *.js src/**/*.js *.json --fix",
    "css": "stylelint src/css/*.css --fix",
    "start": "npm run develop",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
